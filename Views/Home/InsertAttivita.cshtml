@{
    ViewData["Title"] = "Inserisci attività";
}
@model AttivitaModel;
<h1>@ViewData["Title"]</h1>


<div class="container">
    <div class="row">
        <div class="col-12">
            <h3 class="display-4">Inserisci attività</h3>
            Name: <input type="text" id="personName" />
            Last Name: <input type="text" id="personLastName" />
            <button class="btn btn-primary" onclick="insertPerson()">Insert Person</button>
        </div>
    </div>
</div>